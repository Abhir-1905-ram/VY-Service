{
  "expo": {
    "name": "VY Service",
    "slug": "vys",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "androidStatusBar": {
      "backgroundColor": "#1A237E",
      "barStyle": "light-content",
      "translucent": true
    },
    "androidNavigationBar": {
      "backgroundColor": "#1A237E",
      "barStyle": "light-content",
      "visible": "leanback"
    },
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "backgroundColor": "#1A237E",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vyservice.app",
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": false
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1A237E"
      },
      "package": "com.vyservice.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "READ_CONTACTS"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "c139986a-c14b-4570-9f56-f5394b21c6ad"
      }
    }
  }
}
